{
    "name": "supply-chain-fps",
    "version": "1.0.0",
    "client": {
        "organization": "fps",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "fps": {
            "mspid": "FpsMSP",
            "peers": [
                "peer0.fps.example.com"
            ],
            "certificateAuthorities": [
                "ca.fps.example.com"
            ]
        }
    },
    "peers": {
        "peer0.fps.example.com": {
            "url": "grpcs://localhost:7051",
            "tlsCACerts": {
                "pem": ""
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.fps.example.com",
                "hostnameOverride": "peer0.fps.example.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.fps.example.com": {
            "url": "https://localhost:7054",
            "caName": "ca-fps",
            "tlsCACerts": {
                "pem": ""
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
